<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>
<% @products.each do |product| %>
  <div class="col-3">
    <p><%= product.name %></p>
    <%= cl_image_tag product.photo.key, height: 160, width: 160, crop: :fill %>
    <%= link_to "#{product.name}", product_path(product) %>
  </div>
<% end %>
